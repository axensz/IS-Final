<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-50dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="252dp"
            android:layout_height="41dp"
            android:fontFamily="@font/lalezar"
            android:text="Rutinas"
            android:textColor="#FFFFFF"
            android:textSize="36sp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="105dp" />

        <EditText
            android:id="@+id/editTextTextPassword2"
            android:layout_width="244dp"
            android:layout_height="49dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Buscar Rutina"
            android:inputType="textPassword"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="151dp"
            tools:layout_editor_absoluteY="41dp" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="183dp"
            android:layout_height="44dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Crear Rutina"
            android:inputType="textPassword"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="212dp"
            tools:layout_editor_absoluteY="105dp" />

        <EditText
            android:id="@+id/editTextTextPassword16"
            android:layout_width="361dp"
            android:layout_height="81dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Lunes: Pecho - Tricep - Hombro"
            android:inputType="textPassword"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="227dp" />

        <EditText
            android:id="@+id/editTextTextPassword17"
            android:layout_width="363dp"
            android:layout_height="83dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Nombre de Rutina"
            android:inputType="textPassword"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="342dp" />

        <EditText
            android:id="@+id/editTextTextPassword18"
            android:layout_width="361dp"
            android:layout_height="81dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Nombre de Rutina"
            android:inputType="textPassword"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="468dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lalezar"
            android:text="IronInsight"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="167dp"
            tools:layout_editor_absoluteY="4dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@drawable/screenshot_2"
            android:contentDescription="@string/app_name"
            tools:ignore="DuplicateSpeakableTextCheck"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="37dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>