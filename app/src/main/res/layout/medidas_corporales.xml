<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="0dp">

        <View
            android:id="@+id/rectangleView10"
            android:layout_width="290dp"
            android:layout_height="1dp"
            android:background="#5F68C1"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="527dp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="158dp"
            android:layout_height="44dp"
            android:background="@drawable/custom_button"
            android:backgroundTint="#5F68C1"
            android:fontFamily="@font/montserrat"
            android:text="Agregar"
            android:onClick="home"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="126dp"
            tools:layout_editor_absoluteY="569dp" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="344dp"
            android:layout_height="45dp"
            android:fontFamily="@font/lalezar"
            android:text="Ingresa tus medidas corporales"
            android:textColor="#FFFFFF"
            android:textFontWeight="900"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="32dp"
            tools:layout_editor_absoluteY="123dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/screenshot_2"
            android:onClick="regresar4"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="30dp" />

        <EditText
            android:id="@+id/editTextTextPassword13"
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Altura"
            android:inputType="textPassword"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="12sp"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="223dp" />

        <EditText
            android:id="@+id/editTextTextPassword14"
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Peso"
            android:inputType="textPassword"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="12sp"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="324dp" />

        <EditText
            android:id="@+id/editTextTextPassword15"
            android:layout_width="300dp"
            android:layout_height="52dp"
            android:background="@drawable/edittext_border"
            android:backgroundTint="#5F68C1"
            android:ems="10"
            android:fontFamily="@font/montserrat_light"
            android:hint="Porcentaje de grasa corporal"
            android:inputType="textPassword"
            android:textColor="#FFFFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="12sp"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="413dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>